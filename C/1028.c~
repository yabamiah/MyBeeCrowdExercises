#include <stdio.h>

int main()
{
  int casos, pl1, pl2;

  scanf("%d", &casos);

  while (casos--) {
	 
	 scanf("%d %d", &pl1, &pl2);
	 
	 if (pl2 > pl1) {
		int temp = pl1;
		pl1 = pl2;
		pl2 = temp;
	 }

	 int n1 =pl1, n2 = pl2, resto = pl1 % pl2;

	 while (resto!= 0) {
		  1 = pl2;
		pl2 = resto;
		resto = pl1 % pl2;
	 }

	 printf("%d\n", resto);
  }
  
  return 0;
}
